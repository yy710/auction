<view>
  <!--
  <text>{{ socketText }}</text>
  <text>{{ '  carid: ' + carid }}</text>
  -->
  <van-count-down use-slot class="control-count-down" time="{{ time }}" auto-start="{{ false }}" bind:change="onTimeChange" bind:finish="finished">
    <view>
      <text>距离竞价结束还剩余</text>
      <text class="item">{{ timeData.minutes }}</text>
      <text>分</text>
      <text class="item">{{ timeData.seconds }}</text>
      <text>秒</text>
    </view>
  </van-count-down>
  <van-grid clickable column-num="3">
    <van-grid-item bindclick="add200">
      <view slot="text">加价
        <text style="font-size:20px;color:red">¥200</text>元</view>
    </van-grid-item>
    <van-grid-item bindclick="add500">
      <view slot="text">加价
        <text style="font-size:20px;color:red">¥500</text>元</view>
    </van-grid-item>
    <van-grid-item bindclick="add1000">
      <view slot="text">加价
        <text style="font-size:20px;color:red">¥1000</text>元</view>
    </van-grid-item>
  </van-grid>
  <!--
  <van-grid clickable column-num="3">
    <van-grid-item text="开始" icon="play-circle-o" bindclick="start" />
    <van-grid-item text="暂停" icon="pause-circle-o" bindclick="pause" />
    <van-grid-item text="重置" icon="replay" bindclick="reset" />
  </van-grid>
  <navigator url="/pages/upload/upload" open-type="switchTab">跳转到上传页面</navigator>
  -->
</view>